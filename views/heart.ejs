<script type="text/javascript">
    var chartSpeed = Highcharts.chart('heart', Highcharts.merge(gaugeOptions, {
        yAxis: {
            min: 0,
            max: 100,
            title: {
                text: 'Heart Rate'
            }
        },

        credits: {
            enabled: false
        },

        series: [{
            name: 'Speed',
            data: [<%-74-%>],
            dataLabels: {
                format: '<div style="text-align:center"><span style="font-size:25px;color:' +
                ((Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black') + '">{y}</span><br/>' +
                '<span style="font-size:12px;color:silver">Hours</span></div>'
            },
            tooltip: {
//        valueSuffix: ' km/h'
            }
        }]

    }));

</script>
<script type="text/javascript">
    var chartSpeed = Highcharts.chart('fat', Highcharts.merge(gaugeOptions, {
        yAxis: {
            min: 0,
            max: 100,
            title: {
                text: 'Time Asleep'
            }
        },

        credits: {
            enabled: false
        },

        series: [{
            name: 'Speed',
            data: [<%-42-%>],
