<div class="wraper container-fluid">
    <div class="row">
        <div class="col-lg-1">
        </div>
        <div class="col-lg-10">
            <h3><strong>Enter Details to Predict Diabetes</strong></h3>
        <form>
            <div class="form-group">
                <label>Age(Years)</label>
                <input type="text" name="age" class="form-control input-lg" id="age" ng-model="age" placeholder="Eg:45" required>
            </div>
            <div class="form-group">
                <label>Gender</label>
                <select class="form-control input-lg" id="chestpaintype">
                        <option value="1">Male</option>
                        <option value="0">Female</option>
                    </select>
            </div>
            <div class="form-group">
                <label>Number of Times Pregnant (if male enter 0)</label>
                <input type="text" name="age" class="form-control input-lg" id="age" ng-model="preg" required>
            </div>
            <div class="form-group">
                <label> Plasma glucose concentration at 2 hours in an oral glucose tolerance test  </label>
                <input type="text" name="restingbloodpressure" class="form-control input-lg" ng-model="plas" id="restingbloodpressure" required placeholder="Eg:148">
            </div>
            <div class="form-group">
                <label> Diastolic blood pressure (in mm Hg) </label>
                <input type="text" name="restingbloodpressure" class="form-control input-lg" ng-model="diast" id="restingbloodpressure" placeholder="Eg:80" required>
            </div>
            <div class="form-group">
                <label>Triceps skin fold thickness (in mm) </label>
                <input type="text" name="serumcholestrol" class="form-control input-lg" ng-model="tricep" id="serumcholestrol" 
                placeholder="Eg:25" required>
            </div>
            <div class="form-group">
                <label> 2-Hour serum insulin (mu U/ml) </label>
                <input type="text" name="blo" class="form-control input-lg" ng-model="serum" id="serumcholestrol" placeholder="Eg:0.6" required>
            </div>
            <div class="form-group">
                <label>Body mass index</label>
                <input type="text" name="maxheartrate" class="form-control input-lg" ng-model="body" id="maxheartrate" placeholder="Eg:20.2" required>
            </div>
            <div class="form-group">
                <label>Diabetes pedigree function</label>
                <input type="text" name="stdepression" class="form-control input-lg"  ng-model="pede" id="stdepression" 
                placeholder="Eg:0.63" required>
            </div>
            <input type="button" class="btn btn-info btn-lg" value="Get Prediction" ng-click="getPrediction()">
        </form>
