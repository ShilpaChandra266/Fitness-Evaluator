<script>
    Highcharts.chart('calorieComparison', {

        chart: {
            type: 'column'
        },

        title: {
            text: 'Calories In VS. Calories Out'
        },

        xAxis: {
            type: 'category'
        },

        yAxis: {
            allowDecimals: false,
            min: 0,
            title: {
                text: 'Number of calories'
            }
        },

        legend: {
            enabled: false
        },

        tooltip: {
            formatter: function () {
                return '<b>' + this.series.name + ': ' + this.y + '</b><br/>'

            }
        },

        plotOptions: {},

        series: [{
            name: 'Calories',
            colorByPoint: true,
            data: [{
                name: 'Calories In',
                y:  <%- totalCaloriesIn.today %>,
                drilldown: null
            }, {
                name: 'Calories Out',
                y: <%- totalCaloriesOut %>,
                drilldown: 'Calories Out'
            }]
        }],

        drilldown: {
            series: [{
                name: 'Calories Out',
                id: 'Calories Out',
                data: [
                    [
                        "<%- calories[0].dateTime %>",
                        <%- calories[0].value %>
                    ],
                    [
                        "<%- calories[1].dateTime %>",
                        <%- calories[1].value %>
                    ],
                    [
                        "<%- calories[2].dateTime %>",
                        <%- calories[2].value %>
                    ],
                    [
                        "<%- calories[3].dateTime %>",
                        <%- calories[3].value %>
                    ],
                    [
                        "<%- calories[4].dateTime %>",
                        <%- calories[4].value %>
                    ],
                    [
                        "<%- calories[5].dateTime %>",
                        <%- calories[5].value %>
                    ],
                    [
                        "<%- calories[6].dateTime %>",
                        <%- calories[6].value %>
                    ]

                ]
            }]
        }

    });
</script>

<script>

    var data = <%- JSON.stringify(weightArray) %>;

    var weightDates= <%- JSON.stringify(weightDateArray) %>;

    Highcharts.chart('weightChange', {
        chart: {
            type: 'spline'
        },
        title: {
            text: 'Weight Change'
        },

        subtitle: {
            text: 'Total Change this week: '+ <%-weightChange%> +' lb(s)'
        },

        legend: {
            enabled: false
        },

        xAxis: {
            categories: weightDates
        },
        yAxis: {
            title: {
                text: 'Weight'
            },
            labels: {
                formatter: function () {
                    return this.value + 'lb';
                }
            }
        },
        tooltip: {
            crosshairs: true,
            shared: true
        },
        plotOptions: {
            spline: {
                marker: {
                    radius: 4,
                    lineColor: '#666666',
                    lineWidth: 1
                }
            }
        },
        series: [{
            name: 'Weight change from ' + <%= startDate %>+'to' + <%= endDate %>,
            marker: {
                symbol: 'square'
            },
            data: data

        }]
    });
</script>


